	<update id="approvalUpdate" parameterType="sanctnDocVO">
		/* kr.or.ddit.approval.mapper.approvalUpdate */
		UPDATE SANCTN_DOC
		   SET EXPRN_LRN_BGNDE = #{exprnLrnBgnde}
		     , EXPRN_LRN_ENDDE = #{exprnLrnEndde}
		     , LRN_STLE = #{lrnStle}
		     , PURPS = #{purps}
		     , DSTN = #{dstn}
		     , DOC_CN = #{docCn}
		     , RQST_DE = #{rqstDe}
		 WHERE DOC_CODE = #{docCode}
	</update>
	
